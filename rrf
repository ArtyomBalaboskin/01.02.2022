import turtle
t1 = turtle.Turtle()
t1.fillcolor("black")
t1.begin_fill()
t1.color("red")
t1.up()
t1.setpos(-100, 50)
t1.down()
t1.circle(50, steps= 3)


t2 = turtle.Turtle()
t2.color()
t2.up()
t2.setpos(50, 50)
t2.down()
t2.circle(50, steps = 4)

t3 = turtle.Turtle()
t3.color("blue")
t3.up()
t3.setpos(-25, -100)
t3.down()
t3.circle(50)
